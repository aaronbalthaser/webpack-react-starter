{
  "name": "webpack_starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "build": "webpack --config webpack/webpack.production.config.js",
    "dev": "webpack-dev-server --config webpack/webpack.development.config.js",
    "test": "jest --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.7.0",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-remove-object-properties": "1.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "check-prop-types": "1.1.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.4.0",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-enzyme": "7.1.2",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "raw-loader": "3.1.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.0.1",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules", "src", "test"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^@[/](.+)": "<rootDir>/src/$1",
      "^components[/](.+)": "<rootDir>/src/components/$1",
      "^utils[/](.+)": "<rootDir>/test/$1"
    }
  }
}
